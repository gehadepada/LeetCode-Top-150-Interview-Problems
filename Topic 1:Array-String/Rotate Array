class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        int numOfRotation= nums.size()-(k % nums.size());
        vector<int>res;
        for(int i = numOfRotation ; i <nums.size() ; i++)
        {
            res.push_back(nums[i]);
        }
        for(int i = 0 ;i <numOfRotation ; i++)
        {
            res.push_back(nums[i]);
        }
        nums=res;
        
    }
};
