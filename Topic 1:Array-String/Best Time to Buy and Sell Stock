class Solution {
public:
    int maxProfit(vector<int>& prices) {
        vector<int>maxi((prices.size())+1,0);
        int res= 0 ; 
        for(int i = (prices.size())-1 ; i >=0 ; i--)
        {
            maxi[i]=max(prices[i] ,maxi[i+1]);
        }
        for(int i =0 ; i < prices.size() ; i ++)
        {
            res=max(maxi[i+1]-prices[i]  , res);
        }
        return res;
        
    }
};
